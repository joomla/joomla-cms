{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "joomla",
  "version": "4.0.0",
  "description": "Joomla CMS",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "core",
      "type": "script",
      "class": "CoreAssetItem",
      "uri": "system/core.min.js"
    },
    {
      "name": "keepalive",
      "type": "script",
      "class": "KeepaliveAssetItem",
      "dependencies": [
        "core"
      ],
      "uri": "system/keepalive.min.js"
    },
    {
      "name": "template.active",
      "type": "style",
      "uri": "",
      "description": "A dummy asset to allow to extensions to use it as dependency to active template"
    },
    {
      "name": "template.active",
      "type": "script",
      "uri": "",
      "description": "A dummy asset to allow to extensions to use it as dependency to active template"
    },
    {
      "name": "multiselect",
      "type": "script",
      "dependencies": [
        "core"
      ],
      "uri": "system/multiselect.min.js"
    },
    {
      "name": "searchtools",
      "type": "script",
      "dependencies": [
        "core"
      ],
      "uri": "system/searchtools.min.js"
    },
    {
      "name": "searchtools",
      "type": "style",
      "uri": "system/searchtools.css"
    },
    {
      "name": "searchtools",
      "type": "preset",
      "dependencies": [
        "searchtools#style",
        "searchtools#script"
      ]
    },
    {
      "name": "showon",
      "type": "script",
      "dependencies": [
        "core"
      ],
      "uri": "system/showon.min.js"
    },
    {
      "name": "switcher",
      "type": "style",
      "uri": "system/fields/switcher.min.css"
    },
    {
      "name": "form.validate",
      "type": "script",
      "class": "FormValidateAssetItem",
      "dependencies": [
        "core",
        "punycode"
      ],
      "uri": "system/fields/validate.min.js"
    }
  ]
}
