{
    "name": "joomla/joomla-cms",
    "type": "project",
    "description": "Joomla CMS",
    "keywords": [
        "joomla",
        "cms"
    ],
    "homepage": "https://github.com/joomla/joomla-cms",
    "license": "GPL-2.0-or-later",
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": {
            "joomla/application": "dist",
            "joomla/archive": "dist",
            "joomla/authentication": "dist",
            "joomla/console": "dist",
            "joomla/controller": "dist",
            "joomla/crypt": "dist",
            "joomla/data": "dist",
            "joomla/database": "dist",
            "joomla/di": "dist",
            "joomla/event": "dist",
            "joomla/filter": "dist",
            "joomla/http": "dist",
            "joomla/input": "dist",
            "joomla/ldap": "dist",
            "joomla/oauth1": "dist",
            "joomla/oauth2": "dist",
            "joomla/mediawiki": "dist",
            "joomla/registry": "dist",
            "joomla/router": "dist",
            "joomla/session": "dist",
            "joomla/string": "dist",
            "joomla/test": "dist",
            "joomla/uri": "dist",
            "joomla/utilities": "dist"
        },
        "vendor-dir": "libraries/vendor"
    },
    "support": {
        "issues": "https://issues.joomla.org",
        "irc": "irc://chat.freenode.net/joomla/",
        "forum": "https://forum.joomla.org/",
        "docs": "https://docs.joomla.org"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\CMS\\": "libraries/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2.5",
        "joomla/application": "~2.0@dev",
        "joomla/archive": "~2.0@dev",
        "joomla/authentication": "~2.0@dev",
        "joomla/console": "~2.0@dev",
        "joomla/controller": "~2.0@dev",
        "joomla/crypt": "~2.0@dev",
        "joomla/data": "~2.0@dev",
        "joomla/database": "~2.0@dev",
        "joomla/di": "~2.0@dev",
        "joomla/event": "~2.0@dev",
        "joomla/filter": "~2.0@dev",
        "joomla/http": "~2.0@dev",
        "joomla/input": "~2.0@dev",
        "joomla/ldap": "~2.0@dev",
        "joomla/oauth1": "~2.0@dev",
        "joomla/oauth2": "~2.0@dev",
        "joomla/registry": "~2.0@dev",
        "joomla/router": "~2.0@dev",
        "joomla/session": "~2.0@dev",
        "joomla/string": "~2.0@dev",
        "joomla/uri": "~2.0@dev",
        "joomla/utilities": "~2.0@dev",
        "algo26-matthias/idna-convert": "~3.0",
        "defuse/php-encryption": "~2.1",
        "doctrine/inflector": "~1.3",
        "fig/link-util": "~1.0",
        "google/recaptcha": "~1.1",
        "laminas/laminas-diactoros": "^2.2.2",
        "ozdemirburak/iris": "2.0.*",
        "paragonie/sodium_compat": "~1.9.1",
        "phpmailer/phpmailer": "~6.0",
        "psr/link": "~1.0",
        "symfony/console": "~5.0",
        "symfony/error-handler": "~5.0",
        "symfony/ldap": "~5.0",
        "symfony/options-resolver": "~5.0",
        "symfony/polyfill-php73": "^1.10",
        "symfony/web-link": "~5.0",
        "symfony/yaml": "~5.0",
        "typo3/phar-stream-wrapper": "~3.1",
        "wamania/php-stemmer": "^2.0",
        "maximebf/debugbar": "^1.15",
        "tobscure/json-api": "^0.4.1",
        "willdurand/negotiation": "^2.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "psr/log": "~1.0",
        "ext-gd": "*",
        "web-auth/webauthn-lib": "2.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "joomla/cms-coding-standards": "~2.0.0-alpha2@dev",
        "joomla/coding-standards": "~3.0@dev",
        "joomla/mediawiki": "dev-master",
        "friendsofphp/php-cs-fixer": "~2.12",
        "squizlabs/php_codesniffer": "~3.0",
        "joomla-projects/joomla-browser": "~4.0@dev",
        "codeception/codeception": "~4.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.0",
        "joomla/test": "~2.@dev",
        "codeception/module-asserts": "^1.0",
        "codeception/module-db": "^1.0",
        "codeception/module-rest": "^1.0",
        "codeception/module-webdriver": "^1.0",
        "codeception/module-phpbrowser": "^1.0"
    }
}
